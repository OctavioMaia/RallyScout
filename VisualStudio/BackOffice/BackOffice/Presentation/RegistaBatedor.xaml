        <Label x:Name="labelMarca" Content="Nome" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Height="42" Width="86" FontFamily="Arial" FontSize="18" FontWeight="Bold"/>
        <TextBox x:Name="textBoxNome" HorizontalAlignment="Left" Height="27" Margin="122,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="342" FontFamily="Arial" FontSize="16"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Height="27" Margin="122,103,0,0" VerticalAlignment="Top" Width="342"/>
        <Button x:Name="buttonOK" Content="Ok" HorizontalAlignment="Left" Height="30" Margin="10,143,0,0" VerticalAlignment="Top" Width="215" FontSize="18" FontWeight="Bold" Click="buttonOK_Click"/>
