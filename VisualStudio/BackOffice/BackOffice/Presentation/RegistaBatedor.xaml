<Window x:Class="BackOffice.Presentation.RegistaBatedor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BackOffice.Presentation"
        mc:Ignorable="d"
        Title="Registo de Batedor" Height="213.374" Width="488.27" ResizeMode="NoResize" FontFamily="Arial" WindowStartupLocation="CenterScreen" Icon="/BackOffice;component/icon.ico">
    <Grid>
        <Label x:Name="labelModeloVeiculo_Copy1" Content="Email" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="27" Width="86" FontFamily="Arial" FontSize="18" FontWeight="Bold"/>
        <TextBox x:Name="textBoxEmail" HorizontalAlignment="Left" Height="27" Margin="122,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="342" FontFamily="Arial" FontSize="16"/>
